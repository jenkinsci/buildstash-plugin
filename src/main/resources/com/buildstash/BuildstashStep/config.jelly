    <f:entry title="SCM Repo URL" field="vcRepoUrl">
        <f:textbox />
    </f:entry>
    
    <f:entry title="SCM Branch" field="vcBranch">
        <f:textbox />
    </f:entry>
    
    <f:entry title="SCM Commit SHA" field="vcCommitSha">
        <f:textbox />
    </f:entry>
    
    <f:entry title="SCM Commit URL" field="vcCommitUrl">
        <f:textbox />
    </f:entry>
    
    <f:entry title="Platform" field="platform">
        <f:textbox />
    </f:entry>
    
    <f:entry title="Stream" field="stream">
        <f:textbox />
    </f:entry>
    
    <f:entry title="Notes" field="notes">
        <f:textarea />
    </f:entry>
